{
  "name": "NFTrans",
  "description": "Transfer you NFTs on the web3 way!",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "bundle.background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/nftrans@16.png",
      "32": "/images/nftrans@32.png",
      "48": "/images/nftrans@48.png",
      "128": "/images/nftrans@128.png"
    }
  },
  "icons": {
    "16": "/images/nftrans@16.png",
    "32": "/images/nftrans@32.png",
    "48": "/images/nftrans@48.png",
    "128": "/images/nftrans@128.png"
  }
}
